    {
      "name": "Types, Tags",
      "scope": "entity.name.type, entity.other.inherited-class",
      "settings": {
        "foreground": "#8CD8D8",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Module",
      "scope": "entity.name.type.module",
      "settings": {
        "fontStyle": ""
      }
    },
    {
